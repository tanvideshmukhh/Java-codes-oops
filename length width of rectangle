class Rectangle {
    float length;
    float width;

    Rectangle(float length, float width) {
        this.length = length;
        this.width = width;
    }

    void displayDetails() {
        System.out.println("Length: " + length);
        System.out.println("Width: " + width);
    }

    float calculateArea() {
        return length * width;
    }

    public static void main(String[] args) {
        Rectangle rect = new Rectangle(10.5f, 7.2f);
        rect.displayDetails();
        System.out.println("Area: " + rect.calculateArea());
    }
}
